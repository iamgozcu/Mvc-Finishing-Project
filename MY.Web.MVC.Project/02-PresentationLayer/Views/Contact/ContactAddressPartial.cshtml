@model List<_00_DataAccessLayer.Entity.Setting>

@foreach (var item in Model)
{
    <div class="table-form ">

        <div class="field-group">
            <span style="font-size:xx-large">ADRES</span><br />
            <ul class="info-inline">
                <li><i class="fa fa-home"></i>@item.Address</li>
            </ul>
        </div>
        <br />
        <div class="field-group">
            <span style="font-size:xx-large">TELEFON</span><br />
            <ul class="info-inline">
                <li><i class="fa fa-phone"></i>@item.Phone</li>
                <li><i class="fa fa-phone"></i>@item.GSM</li>
            </ul>
        </div>
        <br />
        <div class="field-group">
            <span style="font-size:xx-large">E-POSTA</span><br />
            <ul class="info-inline">
                <li><i class="fa fa-envelope"></i>@item.ContactMailAddress</li>
            </ul>
        </div>
        <br />
        <div class="field-group">
            <span style="font-size:xx-large">Sosyal Medya</span><br />
            <ul class="social-icons pp_middle link-list inline">
                <li>
                    <a href="@item.Facebook" class="fa fa-facebook-square"></a>
                </li>
                <li>
                    <a href="@item.Twitter" class="fa fa-twitter-square"></a>
                </li>
                <li>
                    <a href="@item.Instagram" class="fa fa-instagram"></a>
                </li>
                <li>
                    <a href="@item.Linkedin" class="fa fa-linkedin-square"></a>
                </li>
                <li>
                    <a href="@item.Google" class="fa fa-google-plus-square"></a>
                </li>
            </ul>

        </div>
        <br />
    </div>
}

